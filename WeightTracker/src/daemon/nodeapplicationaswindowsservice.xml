<service>
	<id>nodeapplicationaswindowsservice.exe</id>
	<name>Node application as Windows Service</name>
	<description>Node application as Windows Service</description>
	<executable>C:\Program Files (x86)\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Users\windows\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\SourceCode\node-weight-tracker\src\server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Node application as Windows Service wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>BOOTCAMP-WINDOW</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\SourceCode\node-weight-tracker</workingdirectory>
</service>